{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 800,
  "height": 500,
  "title": "Real Estate Sales in Metropolitan Perth from September 2018 to June 2020",
  "projection": {"type": "mercator"},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/towen123/FIT3179/main/metro_wa.json",
        "format": {"type": "topojson", "feature": "WA_LOCALITY_POLYGON_SHP"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/towen123/FIT3179/main/wa_data.csv"
      },
      "mark": {"type": "circle", "size": 15},
      "encoding": {
        "longitude": {"field": "lon", "type": "quantitative"},
        "latitude": {"field": "lat", "type": "quantitative"},
        "color": {
          "field": "price",
          "type": "quantitative",
          "bin": {"extent": [0, 2500000]},
          "scale": {"scheme": "oranges"},
          "title": "Price in AUD"
        },
        "tooltip": [
          {"field": "price", "type": "quantitative", "title": "Price (AUD)"},
          {"field": "suburb", "title": "Suburb"},
          {"field": "property_type", "title": "Property Type"}
        ]
      }
    }
  ],
  "config": {}
}